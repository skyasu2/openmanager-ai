  "scripts": {
    "docs:check": "bash scripts/docs/check-docs.sh",
    "docs:check:fix": "bash scripts/docs/check-docs.sh --fix",
    "docs:lint": "npx markdownlint-cli2 --config active.markdownlint-cli2.jsonc 'docs/**/*.md' '!docs/analysis/**/*.md' '!docs/reviews/**/*.md' '!docs/status.md'",
    "docs:lint:historical": "npx markdownlint-cli2 --config historical.markdownlint-cli2.jsonc 'docs/analysis/**/*.md' 'docs/reviews/**/*.md' 'docs/status.md'",
    "docs:lint:fix": "npx markdownlint-cli2 --config active.markdownlint-cli2.jsonc 'docs/**/*.md' '!docs/analysis/**/*.md' '!docs/reviews/**/*.md' '!docs/status.md' --fix",
    "docs:lint:changed": "bash scripts/docs/lint-changed.sh",
    "docs:links:internal": "node scripts/docs/check-internal-links.js docs",
    "docs:budget": "node scripts/docs/doc-budget-report.js --write",
    "docs:budget:strict": "node scripts/docs/doc-budget-report.js --write --strict",
    "docs:inventory": "node scripts/docs/generate-inventory.js",
    "docs:check:wsl": "bash scripts/wsl/docs-management-check.sh",
    "docs:check:wsl:strict": "bash scripts/wsl/docs-management-check.sh --strict",
    "docs:toc": "npx doctoc docs/README.md --github --notitle",
    "docs:links": "find docs -name '*.md' -type f | head -20 | xargs npx markdown-link-check -q",
    "docs:links:full": "find docs -name '*.md' -type f | xargs npx markdown-link-check -q",
    "claude:sanitize": "node scripts/diagnostics/claude-json-sanitize.js --write",
    "ai:bridge": "bash scripts/ai/agent-bridge.sh",
    "codex:local": "bash scripts/mcp/codex-local.sh",
    "codex:local:storybook": "OPENMANAGER_STORYBOOK_MCP_MODE=on bash scripts/mcp/codex-local.sh",
    "mcp:env:codex": "bash scripts/mcp/resolve-runtime-env.sh --print",
    "mcp:status": "claude mcp list",
    "mcp:status:codex": "bash scripts/mcp/codex-local.sh mcp list",
    "mcp:status:codex:storybook": "OPENMANAGER_STORYBOOK_MCP_MODE=on bash scripts/mcp/codex-local.sh mcp list",
    "mcp:doctor:codex": "bash scripts/mcp/resolve-runtime-env.sh --print && bash scripts/mcp/mcp-health-check-codex.sh",
    "mcp:health:codex": "bash scripts/mcp/mcp-health-check-codex.sh",
    "mcp:playwright:mode:windows": "bash scripts/mcp/set-playwright-mcp-mode.sh --mode windows-http --port 8931",
    "mcp:playwright:mode:stdio": "bash scripts/mcp/set-playwright-mcp-mode.sh --mode stdio",
    "mcp:playwright:windows:start": "bash scripts/mcp/start-playwright-mcp-windows.sh --port 8931 --browser msedge",
    "mcp:playwright:windows:enable": "bash scripts/mcp/enable-playwright-headed-windows.sh --port 8931 --browser msedge",
    "stitch:check": "node scripts/stitch/validate-stitch-registry.js",
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev -p 3000",
    "dev:gemini": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev -p 3004",
    "dev:ag": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev -p 3005",
    "dev:network": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev -p 3000 --hostname 0.0.0.0",
    "dev:stable": "cross-env NEXT_DISABLE_DEVTOOLS=1 NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:playwright": "cross-env NEXT_DISABLE_DEVTOOLS=1 NEXT_PUBLIC_E2E_TESTING=true NODE_OPTIONS='--max-old-space-size=4096' next dev -p 3000",
    "data:fix": "npx tsx scripts/data/otel-fix.ts",
    "data:verify": "npx tsx scripts/data/otel-verify.ts",
    "data:precomputed:build": "npx tsx cloud-run/ai-engine/scripts/build-precomputed-states.ts",
    "grafana:export": "tsx scripts/grafana/otlp-export.ts --all",
    "grafana:export:hour": "tsx scripts/grafana/otlp-export.ts --hour",
    "grafana:push": "tsx scripts/grafana/otlp-export.ts --push --all",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=2048' next build",
    "build:prod": "cross-env NODE_OPTIONS='--max-old-space-size=2048' NEXT_DISABLE_DEVTOOLS=1 next build",
    "build:ci": "npm run build:prod",
    "build:vercel": "npm run build:ci && npm run lint:ci && npm run test:quick",
    "deploy:safe": "npm run build:ci && npm run lint:ci && npm run test:quick && npx vercel --prod",
    "env:sync:production": "bash scripts/env/sync-vercel.sh production",
    "env:sync:preview": "bash scripts/env/sync-vercel.sh preview",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "check": "bash scripts/dev/biome-wrapper.sh check .",
    "lint": "bash scripts/dev/biome-wrapper.sh lint .",
    "lint:fix": "bash scripts/dev/biome-wrapper.sh check --write .",
    "lint:strict": "bash scripts/dev/biome-wrapper.sh lint --error-on-warnings .",
    "lint:progressive": "bash scripts/dev/biome-wrapper.sh lint .",
    "lint:fast": "bash scripts/dev/biome-wrapper.sh lint .",
    "lint:ci": "bash scripts/dev/biome-wrapper.sh lint .",
    "lint:ci:strict": "bash scripts/dev/biome-wrapper.sh lint --error-on-warnings .",
    "lint:quick": "bash scripts/dev/biome-wrapper.sh lint .",
    "format": "bash scripts/dev/biome-wrapper.sh format --write .",
    "format:check": "bash scripts/dev/biome-wrapper.sh format .",
    "test": "vitest run --config config/testing/vitest.config.main.ts",
    "test:quick": "vitest run --config config/testing/vitest.config.minimal.ts",
    "test:ci": "vitest run --config config/testing/vitest.config.ci.ts",
    "test:coverage": "vitest run --config config/testing/vitest.config.simple.ts --coverage",
    "test:coverage:full": "vitest run --config config/testing/vitest.config.main.ts --coverage.enabled=true",
    "test:watch": "vitest --config config/testing/vitest.config.simple.ts",
    "test:perf:vitest": "RUN_SLOW_TESTS=true vitest run --config config/testing/vitest.config.main.ts tests/performance/web-vitals-integration.test.ts",
    "test:external-connectivity": "RUN_EXTERNAL_CONNECTIVITY_TESTS=true vitest run --config config/testing/vitest.config.main.ts tests/integration/external-services-connection.test.ts",
    "test:cloud-contract": "vitest run --config config/testing/vitest.config.main.ts tests/api/cloud-run-contract.test.ts",
    "test:e2e": "playwright test tests/e2e/smoke.spec.ts tests/e2e/guest.spec.ts tests/e2e/accessibility.spec.ts tests/e2e/dashboard-server-cards.spec.ts tests/e2e/dashboard-ai-sidebar.spec.ts tests/e2e/login.spec.ts --grep-invert '@external' --config playwright.config.ts",
    "test:e2e:all": "playwright test --config playwright.config.ts",
    "test:e2e:smoke": "playwright test tests/e2e/smoke.spec.ts --grep-invert '@external' --config playwright.config.ts",
    "test:e2e:external": "playwright test --grep '@external' --pass-with-no-tests --config playwright.config.ts",
    "test:e2e:no-ai": "playwright test --grep-invert '@ai-test|@external' --config playwright.config.ts",
    "test:e2e:ai-only": "echo \"No AI-only Playwright suite. Use 'npm run test:contract' for AI regression checks.\"",
    "test:e2e:critical": "playwright test tests/e2e/smoke.spec.ts tests/e2e/critical-auth.spec.ts --grep-invert '@external' --config playwright.config.ts",
    "test:e2e:critical:auth": "playwright test tests/e2e/critical-auth.spec.ts --grep-invert '@external' --config playwright.config.ts",
    "test:e2e:visual": "playwright test tests/e2e/visual.spec.ts --config playwright.config.ts",
    "test:e2e:visual:update": "playwright test tests/e2e/visual.spec.ts --config playwright.config.ts --update-snapshots",
    "test:e2e:with-server": "PORT=3000 npm run dev:stable & sleep 5 && playwright test --config playwright.config.ts; kill %1",
    "test:contract": "vitest run --config config/testing/vitest.config.contract.ts",
    "test:cloud:essential": "node scripts/test/cloud-deploy-essential-smoke.mjs",
    "test:cloud:essential:strict": "node scripts/test/cloud-deploy-essential-smoke.mjs --require-auth",
    "test:cloud:essential:llm-once": "node scripts/test/cloud-deploy-essential-smoke.mjs --require-auth --with-supervisor-call",
    "qa:record": "node scripts/qa/record-qa-run.js",
    "qa:status": "node scripts/qa/print-qa-status.js",
    "test:fast": "npm run test",
    "test:super-fast": "npm run test:quick",
    "test:gate": "npm run test:quick && npm run type-check && npm run lint",
    "lighthouse:vercel": "lighthouse \"https://openmanager-ai-jns171qwy-skyasus-projects.vercel.app\" --output=json --output-path=./reports/lighthouse-vercel.json --chrome-flags=\"--headless\"",
    "test:dev": "npm run test:quick",
    "lighthouse:local": "NODE_ENV=production npm run build && npm run start & sleep 5 && lhci collect --url=http://localhost:3000 && kill %1",
    "type-check": "node scripts/dev/tsc-wrapper.js --noEmit",
    "line-guard": "node scripts/dev/line-count-guard.js --warn 500 --fail 800",
    "line-guard:all": "node scripts/dev/line-count-guard.js --warn 500 --fail 800 --include-tests --include-data",
    "type-check:changed": "bash scripts/dev/typecheck-changed.sh",
    "lint:changed": "bash scripts/dev/lint-changed.sh",
    "lint:incremental": "npm run validate:changed",
    "check:incremental": "npm run validate:changed",
    "validate:changed": "npm run lint:changed && npm run type-check:changed",
    "prepare": "husky || true",
    "validate": "npm run type-check && npm run check && npm run line-guard",
    "validate:quick": "npm run type-check && npm run check && npm run line-guard",
    "validate:all": "npm run type-check && npm run check && npm run line-guard && npm run test",
    "deploy:check": "npm run type-check && npm run test:quick",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "release": "commit-and-tag-version",
    "release:patch": "commit-and-tag-version --release-as patch",
    "release:minor": "commit-and-tag-version --release-as minor",
    "release:major": "commit-and-tag-version --release-as major",
    "release:first": "commit-and-tag-version --first-release",
    "release:dry-run": "commit-and-tag-version --dry-run",
    "release:publish": "bash scripts/release/publish.sh",
    "release:publish:patch": "bash scripts/release/publish.sh patch",
    "release:publish:minor": "bash scripts/release/publish.sh minor",
    "release:publish:major": "bash scripts/release/publish.sh major",
    "release:check": "node scripts/release/check-release-consistency.js",
    "ci": "npm run validate:all",
    "bundle:analyze": "cross-env ANALYZE=true NODE_OPTIONS='--max-old-space-size=2048' next build --webpack",
    "git:pre-commit": "lint-staged",
    "git:pre-push": "npm run validate",
    "hook:pre-commit": "bash scripts/dev/biome-wrapper.sh check --write --staged --no-errors-on-unmatched",
    "hook:pre-push": "node scripts/hooks/pre-push.js",
    "hook:post-commit": "node scripts/hooks/post-commit.js",
    "review:show": "npm run lint && npm run type-check && echo 'âœ… All checks pass. Use /review in Claude Code for full analysis.'",
    "review:status": "ps aux | grep '[p]ost-commit-background.sh' > /dev/null && echo 'ðŸ”„ AI Review is running...' || echo 'âœ… AI Review is NOT running.'",
    "check:usage": "node scripts/check-free-tier-usage.js",
    "perf:analyze": "npm run test:perf:vitest",
    "perf:report": "npm run test:perf:vitest",
    "perf:react-analyze": "echo \"Removed legacy performance analyzer script. Use npm run test:perf:vitest.\"",
    "perf:react-optimize": "echo \"Removed legacy performance optimizer script. Use targeted refactors with benchmark evidence.\"",
    "perf:react-optimize:dry": "echo \"Removed legacy performance optimizer script. Use targeted refactors with benchmark evidence.\"",
    "perf:lint-benchmark": "npm run lint",
    "perf:precommit-benchmark": "npm run test:quick",
    "perf:all-benchmarks": "npm run perf:lint-benchmark && npm run perf:precommit-benchmark",
    "perf:quick": "npm run test:perf:vitest",
    "vitals:integration": "WEB_VITALS_SUPPRESS_LOGS=1 RUN_SLOW_TESTS=true vitest run --config config/testing/vitest.config.main.ts tests/performance/web-vitals-integration.test.ts --reporter=verbose",
    "vitals:e2e": "npm run test:e2e:critical",
    "vitals:mock": "npm run test tests/performance/core-web-vitals.test.ts",
    "vitals:all": "npm-run-all --parallel vitals:integration vitals:e2e vitals:mock",
    "vitals:api-test": "curl -X GET http://localhost:3000/api/web-vitals && echo",
    "vitals:playwright-integration": "npm run test:e2e:critical",
    "security:audit": "npm audit --audit-level=moderate && claude /security-review && node scripts/env/check-hardcoded-secrets.js",
    "ai-tools:check": "bash scripts/ai/health/check-ai-tools-updates.sh",
    "env:check": "ts-node scripts/env/check-env.ts",
    "dev:tmux:clean": "tmux kill-session -t openmanager-dev",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
