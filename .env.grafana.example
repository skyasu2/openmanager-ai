# Grafana Cloud OTLP Configuration
# Copy to .env.grafana and fill in your values

# OTLP Endpoint (from Grafana Cloud -> Configuration -> OTLP)
GRAFANA_OTLP_ENDPOINT=https://otlp-gateway-prod-ap-southeast-0.grafana.net/otlp

# Instance ID (from Grafana Cloud -> My Account)
GRAFANA_INSTANCE_ID=

# API Token (from Grafana Cloud -> Security -> API Keys)
GRAFANA_API_TOKEN=
